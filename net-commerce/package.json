{
  "name": "oxil-net-commerce",
  "version": "1.0.0",
  "description": "Netistrar E Commerce Theme",
  "main": "index.js",
  "watch": {
    "hugo:watch": {
      "patterns": [
        "hugo",
        "../net1/hugo"
      ],
      "extensions": "html,md",
      "quiet": false
    }
  },
  "scripts": {
    "hugo:watch": "mkdir -p build; rsync -r ../net1/hugo/* build; rsync -r hugo/* build",
    "watch:webpack": "webpack --config webpack.config.js --watch",
    "watch:npm": "npm-watch hugo:watch",
    "watch": "run-p --print-label watch:**"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^13.7.4",
    "cookie-notice": "^1.2.4",
    "css-loader": "^3.4.2",
    "highlight.js": "^9.18.1",
    "lunr": "^2.3.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "query-string": "^6.11.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tippy.js": "^5.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "uglify-template-string-loader": "^1.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
